{
  "name": "h5-framework-ng",
  "version": "1.3.0",
  "description": "lechebang admin panel front-end framework",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.0"
  },
  "license": "MIT",
  "scripts": {
    "precommit": "npm run lint-staged",
    "ng": "ng",
    "start": "ng serve -o",
    "hmr": "ng serve -c=hmr",
    "build": "ng build --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{framework}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false"
  },
  "dependencies": {
    "@angular/animations": "~6.1.0",
    "@angular/common": "~6.1.0",
    "@angular/compiler": "~6.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~6.1.0",
    "@angular/http": "~6.1.0",
    "@angular/platform-browser": "~6.1.0",
    "@angular/platform-browser-dynamic": "~6.1.0",
    "@angular/router": "~6.1.0",
    "@ngx-translate/core": "9.0.0",
    "@ngx-translate/http-loader": "2.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.1",
    "deep-extend": "0.5.1",
    "echarts": "4.1.0",
    "file-saver": "1.3.3",
    "html2canvas": "^1.0.0-alpha.12",
    "js-sha1": "0.6.0",
    "lodash": "^4.17.11",
    "moment": "2.22.1",
    "ng-zorro-antd": "^1.8.1",
    "ngx-countdown": "~3.0.0",
    "ngx-quill": "^3.4.0",
    "qiniu-js": "^2.5.1",
    "quill": "^1.3.6",
    "rxjs": "~6.2.0",
    "rxjs-compat": "~6.2.0",
    "sa-sdk-javascript": "^1.12.10",
    "screenfull": "3.3.1",
    "simple-line-icons": "2.4.1",
    "web-animations-js": "^2.3.1",
    "ws": "^7.0.0",
    "xlsx": "^0.13.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.2",
    "@angular/cli": "~6.1.0",
    "@angular/compiler-cli": "~6.1.0",
    "@angular/language-service": "~6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.2",
    "@types/mockjs": "^1.0.0",
    "@types/node": "^6.0.112",
    "@types/z-schema": "^3.16.32",
    "adm-zip": "^0.4.13",
    "codecov": "^3.0.0",
    "codelyzer": "~4.0.1",
    "gulp": "^3.9.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sauce-launcher": "^1.2.0",
    "less": "~2.7.0",
    "less-loader": "~4.0.0",
    "lint-staged": "^5.0.0",
    "mockjs": "^1.0.1-beta3",
    "protractor": "^5.3.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "through2": "^2.0.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.7.2",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "lint-staged": {
    "framework/**/*.ts": "lint:ts",
    "framework/**/*.less": "lint:style"
  }
}
